<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_proforma_customer_invoice" model="ir.sequence">
            <field name="name">Proforma invoices sequence</field>
            <field name="code">account.invoice.proforma</field>
            <field name="prefix">PROFORMA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>